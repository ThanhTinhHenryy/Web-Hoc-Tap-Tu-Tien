  <ellipse cx="215" cy="45" rx="20" ry="12" fill="#FFFFFF" opacity="0.9"/>
  
  <!-- Núi xa -->
  <polygon points="0,150 50,80 100,130 150,70 200,120 250,60 300,150" fill="#6B8E23"/>
  
  <!-- Núi gần -->
  <polygon points="0,150 30,100 70,130 120,90 170,140 220,100 270,130 300,150" fill="#556B2F"/>
  
  <!-- Tuyết trên đỉnh núi -->
  <polygon points="45,85 50,80 55,85" fill="#FFFFFF"/>
  <polygon points="145,75 150,70 155,75" fill="#FFFFFF"/>
  <polygon points="245,65 250,60 255,65" fill="#FFFFFF"/>
  
